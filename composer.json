{
    "name": "wdmg/yii2-news",
    "description": "News manager",
    "keywords": [
        "yii2",
        "yii2-news",
        "news",
        "wdmg"
    ],
    "type": "yii2-extension",
    "license": "MIT",
    "version": "1.1.4",
    "homepage": "https://github.com/wdmg/yii2-news",
    "support": {
        "source": "https://github.com/wdmg/yii2-news",
        "issues": "https://github.com/wdmg/yii2-news/issues"
    },
    "authors": [
        {
            "name": "Alexsander Vyshnyvetskyy",
            "email": "alex.vyshnyvetskyy@gmail.com"
        },
        {
            "name": "W.D.M.Group, Ukraine",
            "email": "wdmg.com.ua@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "^2.0.40",
        "wdmg/yii2-base": "^1.3.0",
        "wdmg/yii2-selectinput": "^1.0.9",
        "wdmg/yii2-helpers": "^1.4.2",
        "wdmg/yii2-editor": "^1.0.10",
        "wdmg/yii2-widgets": "^1.0.4"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "~2.0.0",
        "codeception/module-phpbrowser": "^1.0.0",
        "codeception/module-webdriver": "^1.1",
        "codeception/module-asserts": "^1.0.0"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "wdmg\\news\\": ""
        }
    },
    "extra": {
        "options": {
            "routePrefix": "admin",
            "baseRoute": "/news",
            "baseLayout": "@app/views/layouts/main",
            "imagePath": "/uploads/news",
            "supportLocales": [
                "ru-RU",
                "uk-UA",
                "en-US"
            ]
        }
    }
}